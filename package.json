{
  "name": "shareable-asset-api",
  "version": "0.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && tsc",
    "build:tests": "rm -rf test/build && tsc -p test/",
    "start": "node build/server.js",
    "test": "npm run build:tests && nyc ava --verbose test/build/test/*_test.js"
  },
  "nyc": {
    "exclude": [
      "build/**",
      "cdk/**",
      "test/build/test/**",
      "**/*.d.ts"
    ],
    "reporter": [
      "json",
      "text"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/config": "3.3.0",
    "@types/node-fetch": "^2.6.2",
    "ava": "5.1.0",
    "babel-register": "6.26.0",
    "nyc": "15.1.0",
    "tslint": "5.20.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@eight/eight-rest": "0.3.3",
    "@eight/logging": "1.5.0",
    "@eight/practices": "6.22.32",
    "@eight/promises": "1.1.1",
    "canvas": "^2.10.2",
    "node-fetch": "3.3.0",
    "types-joi": "2.1.0"
  }
}
