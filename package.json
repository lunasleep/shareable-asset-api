{
  "name": "shareable-asset-api",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && tsc",
    "build:tests": "rm -rf test/build && tsc -p test/",
    "start": "node build/index.js",
    "test": "npm run build:tests && nyc ava --verbose test/build/test/*_test.js"
},
  "author": "",
  "license": "ISC",
  "nyc": {
    "exclude": [
        "build/**",
        "test/build/test/**",
        "**/*.d.ts"
    ],
    "reporter": [
        "json",
        "text"
    ],
    "all": true
},
  "devDependencies": {
    "@types/config": "0.0.33",
    "ava": "0.24.0",
    "babel-register": "6.26.0",
    "nyc": "11.4.1",
    "tslint": "5.9.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@eight/eight-rest": "0.3.3",
    "@eight/logging": "1.4.0",
    "@eight/practices": "6.22.12",
    "@eight/promises": "1.1.1",
    "canvas": "^2.10.1"
  }
}
